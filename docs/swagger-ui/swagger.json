    "paths": {
        "/api/filter_records": {
            "post": {
                "description": "filter records by date and total count range",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "filters"
                ],
                "summary": "Filter Records",
                "parameters": [
                    {
                        "description": "Date and Total Count Range",
                        "name": "body",
