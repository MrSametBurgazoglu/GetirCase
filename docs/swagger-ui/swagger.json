                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/RecordRequests.FilterInput"
                        }
                    }
                ],
                "responses": {}
            }
        }
    },
    "definitions": {
        "RecordRequests.FilterInput": {
            "type": "object",
            "required": [
                "endDate",
                "maxCount",
                "minCount",
                "startDate"
            ],
            "properties": {
                "endDate": {
                    "type": "string",
                    "example": "2018-02-02"
                },
                "maxCount": {
                    "type": "integer",
                    "example": 3000
                },
                "minCount": {
                    "type": "integer",
                    "example": 2700
                },
                "startDate": {
                    "type": "string",
                    "example": "2016-01-26"
                }
            }
        }
    }
